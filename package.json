{
  "name": "bloodyowl-upgrade-reason-react",
  "version": "1.0.0",
  "description": "An alternative upgrade script to ReasonReact 0.7.0",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": { "bin": [ "Upgrade" ] },
    "install": [
      "esy-installer bloodyowl-upgrade-reason-react.install",
      "esy-installer bloodyowl-upgrade-reason-react-tests.install"
    ]
  },
  "scripts": { "test": "esy x RunTests.exe", "doc": "esy dune build @doc" },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/dune": "^1.9.3",
    "@opam/reason": "3.6.2",
    "@reason-native/console": "*",
    "@reason-native/pastel": "*",
    "@reason-native/rely": "*",
    "ocaml": "~4.6.0",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "@opam/odoc": "*",
    "ocaml": "~4.6.0"
  }
}
